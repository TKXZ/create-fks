{
  "name": "create-fks",
  "version": "0.0.2-9",
  "type": "module",
  "bin": {
    "create-fks": "index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "template-*/*",
    "dist",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TKXZ/create-fks.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "unbuild --stub",
    "prebuild": "npm run clean",
    "build": "unbuild",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "cz": "cz"
  },
  "author": "Shenze_Wang",
  "license": "MIT",
  "description": "A simple CLI tool to create Vue project template",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "inquirer": "^10.2.2",
    "minimist": "^1.2.8",
    "ora": "^8.1.0",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^9.0.7",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.5.4",
    "@types/signale": "^1.4.7",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.2.1",
    "husky": "^9.1.6",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz-config.cjs"
    }
  }
}
